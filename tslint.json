{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "curly": [
            true,
            "as-needed"
        ],
        "semicolon": [
            true,
            "never"
        ],
        "no-console": false,
        "one-variable-per-declaration": false,
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ]
    },
    "rulesDirectory": []
}